{
    "presets": [
        "@babel/typescript"
    ],
    "plugins": [
        [
            "@babel/proposal-class-properties",
            {
                "loose": true
            }
        ],
        "@babel/proposal-object-rest-spread",
        [
            "babel-plugin-transform-async-to-promises",
            {
                "inlineHelpers": true
            }
        ]
    ]
}